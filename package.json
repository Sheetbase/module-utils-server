{
	"name": "@sheetbase/utils-server",
	"version": "0.0.5",
	"description": "Useful utilities for Sheetbase backend app.",
	"author": "Sheetbase",
	"homepage": "https://sheetbase.net",
	"license": "MIT",
	"scripts": {
		"build:code": "sheetbase-app-scripts build",
		"build:readme": "sheetbase-app-scripts readme",
		"build:docs": "typedoc ./src --out ./docs --mode file --target ES6 --excludeExternals --excludeNotExported --ignoreCompilerErrors",
		"build": "npm run build:code && npm run build:readme && npm run build:docs",
		"push": "sheetbase-app-scripts push"
	},
	"keywords": [
		"google-apps-script",
		"utilities",
		"sheetbase"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/sheetbase/utils-server.git"
	},
	"bugs": {
		"url": "https://github.com/sheetbase/utils-server/issues"
	},
	"dependencies": {},
	"devDependencies": {
		"@sheetbase/app-scripts": "0.0.14",
		"@types/google-apps-script": "0.0.28"
	}
}
